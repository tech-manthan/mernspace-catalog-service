{
  "name": "catalog-service",
  "version": "1.0.0",
  "description": "Catalog Service for MernSpace",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "test:watch": "jest --watch --runInBand",
    "test": "jest --runInBand",
    "format:check": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "lint:check": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prepare": "husky"
  },
  "author": "Manthan Sharma <techmanthansharma@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-fileupload": "^1.5.1",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "lint-staged": {
    "*.ts": [
      "npm run format:fix",
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/supertest": "^6.0.3",
    "cloudinary": "^2.7.0",
    "config": "^4.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-jwt": "^8.5.1",
    "express-validator": "^7.2.1",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jwks-rsa": "^3.2.0",
    "mongoose": "^8.18.1",
    "mongoose-aggregate-paginate-v2": "^1.1.4",
